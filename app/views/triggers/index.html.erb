<p id="notice"><%= notice %></p>

<h1>Listing Triggers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @triggers.each do |trigger| %>
      <tr>
        <td><%= trigger.name %></td>
        <td><%= trigger.description %></td>
        <td><%= trigger.json %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>